{
  "name": "chat-global-desktop-web-app",
  "version": "1.0.0",
  "description": "Chat Global Desktop Web App",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:dev": "yarn nodemon src/index.ts",
    "lint": "yarn eslint .",
    "fix": "yarn eslint --fix .",
    "pretty": "yarn prettier --write .",
    "postinstall": "yarn run husky install",
    "commit": "lint-staged && yarn git-cz",
    "translate": "ts-node src/translator.ts",
    "check": "yarn run ncu -p yarn --color",
    "update": "yarn run ncu -i -p yarn --color"
  },
  "keywords": [],
  "author": "tnfAngel",
  "license": "ISC",
  "dependencies": {
    "csso": "^5.0.1",
    "express": "^4.17.2",
    "htmlclean": "^3.0.8",
    "morgan": "^1.10.0",
    "request-ip": "^2.1.3",
    "uglify-js": "^3.14.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/eslint": "^8.2.1",
    "@types/node": "^17.0.0",
    "@types/prettier": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.0.5",
    "prettier": "2.5.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint --fix \"src/**/*.{ts,tsx}\"",
      "eslint \"src/**/*.{ts,tsx}\""
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "yarn@3.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chat-Global/desktop-web-app.git"
  },
  "bugs": {
    "url": "https://github.com/Chat-Global/desktop-web-app/issues"
  },
  "homepage": "https://github.com/Chat-Global/desktop-web-app#readme"
}
